<Window x:Class="DataBase_Hospital.AddNewShedule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBase_Hospital"
        mc:Ignorable="d"
        Title="AddNewShedule" Height="300" Width="300">
    <Window.Resources>
        <ResourceDictionary Source="Style.xaml"></ResourceDictionary>
    </Window.Resources>
    <StackPanel Orientation="Vertical">
        <Label>Рабочие дни</Label>
        <TextBox x:Name="TextBoxWorkDays"/>
        <Label>Выходные дни</Label>
        <TextBox x:Name="TextBoxWeekend"/>
        <Label>Примечание</Label>
        <TextBox x:Name="TextBoxNote"/>
        <Button Content="Добавить"
                Click="ButtonBase_OnClick"/>
    </StackPanel>
</Window>
